<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.lockerz.service.user.models.TokenAuthModel" table="user_app_auth">
    	<property name="uid">
        	<column name="uid"/>
    	</property>
    	<property name="apiKey">
        	<column name="api_key"/>
    	</property>
    	<property name="token">
        	<column name="token"/>
    	</property>
	</class>
</hibernate-mapping>